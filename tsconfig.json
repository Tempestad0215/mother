{
    "compilerOptions":{
        "allowJs":true,
        "module":"ESNext",
        "moduleResolution":"Node",
        "target":"esnext",
        "jsx":"preserve",
        "strict":true,
        "esModuleInterop":true,
        "skipLibCheck":true,
        "forceConsistentCasingInFileNames":true,
        "noEmit":true,
        "isolatedModules":true,
        "types": ["vite/client","vue-sweetalert2"],
        "baseUrl": ".",
        "paths": {
            "@components/*" : ["resources/js/Components/*"],
            "@layout/*" : ["resources/js/Layouts/*"],
            "@/*" : ["resources/js/*"],
        }
    },
    "include":[
        "resources/js/**/*.ts",
        "resources/js/**/*.d.ts",
        "resources/js/**/*.vue",
    ]
}
